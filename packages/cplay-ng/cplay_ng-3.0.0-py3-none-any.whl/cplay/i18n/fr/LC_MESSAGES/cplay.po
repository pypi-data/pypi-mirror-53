#: cplay/cplay.py:771
msgid ""
" Global\n"
" ------\n"
" Up, k, C-p   : move to previous item\n"
" Down, j, C-n : move to next item\n"
" PageUp, K    : move to previous page\n"
" PageDown, J  : move to next page\n"
" Home, g      : move to top\n"
" End, G       : move to bottom\n"
" Enter        : chdir or play\n"
" Tab          : switch between filelist/playlist\n"
" n, p         : next/prev track\n"
" z, x         : toggle pause/stop\n"
"\n"
" Left, C-f    : seek backward\n"
" Right, C-b   : seek forward\n"
" C-a, C-e     : restart/end track\n"
" C-s, /       : forward isearch\n"
" C-r, ?       : backword isearch\n"
" C-g, Esc     : cancel\n"
" 1..9, +, -   : volume control\n"
" c, v         : counter/volume mode\n"
" <, >         : horizontal scrolling\n"
" C-l          : refresh\n"
" l            : list mode\n"
" h            : help\n"
" q, Q         : quit?, Quit!\n"
"\n"
" t, T         : tag current/regex\n"
" u, U         : untag current/regex\n"
" Sp, i        : invert current/all\n"
"\n"
" Filelist\n"
" --------\n"
" a            : add (tagged) to playlist\n"
" s            : recursive search\n"
" BS, o        : go to parent/specified dir\n"
"\n"
" Playlist\n"
" --------\n"
" d, D         : delete (tagged) items/all\n"
" m, M         : move (tagged) tracks after/before\n"
" r, R         : toggle repeat/Random mode\n"
" s, S         : shuffle/Sort playlist\n"
" w            : write playlist to file\n"
" @            : jump to current track\n"
msgstr ""
" Global"
" ------"
" Haut, k, C-p : aller au item précédent\n"
" Bas, j, C-n  : aller au item suivant\n"
" PageUp, K    : aller au page précédent\n"
" PageDown, J  : aller au page suivant\n"
" Home, g      : aller en haut\n"
" End, G       : aller au bas\n"
" Entrez       : chdir ou jouez\n"
" Onglet       : liste de fichiers / lecture\n"
" n, p         : piste suivante / précédente\n"
" z, x         : bascule pause / arrêt\n"
"\n"
" Left, C-f    : chercher en avant\n"
" Right, C-b:  : chercher en arrière\n"
" C-a, C-e     : playlist de reprise / fin de piste\n"
" C-s, /       : isearch en avant\n"
" C-r, ?       : isearch en arrière\n"
" C-g, Esc     : annuler\n"
" 1..9, +, -   : contrôle du volume\n"
" c, v         : mode compteur / volume\n"
" <, >         : défilement horizontal\n"
" C-l          : rafraîchir\n"
" l            : mode liste\n"
" h            : aide\n"
" q, Q         : quitte?, quitte!\n"
"\n"
" t, T         : tag courant / regex\n"
" u, U         : décocher le courant / regex\n"
" Sp, i        : inverser le courant / tous\n"
"\n"
" Liste de fichiers\n"
" -----------------\n"
" a            : ajouter (étiquetées) à la liste de lecture\n"
" s            : recherche récursive\n"
" BS, o        : goto parent / répertoire spécifié\n"
"\n"
" Liste de lecture\n"
" ----------------\n"
" d, D         : supprimer (étiquetées) / la liste de lecture\n"
" m, M         : déplace (étiquetées) après / avant\n"
" r, R         : mode répétition / aléatoire\n"
" s, S         : lecture aléatoire / triée\n"
" w            : écrire une liste de lecture\n"
" @            : passer à actif\n"

