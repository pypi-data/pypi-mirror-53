source $(vimrc systemPath)
nnoremap <silent> <Leader>$(sendKey default) :exe 'sil w !python3 -S $(sendblock)' string(bufname("%")) 'default' line('.') line('.')<CR>
nnoremap <silent> <Leader>$(sendKey alternate) :exe 'sil w !python3 -S $(sendblock)' string(bufname("%")) 'alternate' line('.') line('.')<CR>
vnoremap <silent> <Leader>$(sendKey default) :<C-U>exe 'sil w !python3 -S $(sendblock)' string(bufname("%")) 'default' line("'<") line("'>")<CR>
vnoremap <silent> <Leader>$(sendKey alternate) :<C-U>exe 'sil w !python3 -S $(sendblock)' string(bufname("%")) 'alternate' line("'<") line("'>")<CR>$try[$.(
source $(vimrc userPath)) $.()]
