{
    "name": "featuretools.dfs",
    "contributors": [
        "Carles Sala <csala@csail.mit.edu>"
    ],
    "documentation": "https://docs.featuretools.com/generated/featuretools.dfs.html#featuretools.dfs",
    "description": "Calculates a feature matrix and features given a dictionary of entities and a list of relationships.",
    "classifiers": {
        "type": "preprocessor",
        "subtype": "feature_extractor"
    },
    "modalities": [],
    "primitive": "mlprimitives.adapters.featuretools.DFS",
    "fit": {
        "method": "dfs",
        "args": [
            {
                "name": "X",
                "default": null,
                "type": "DataFrame"
            },
            {
                "name": "target_entity",
                "default": null,
                "type": "str"
            },
            {
                "name": "entityset",
                "default": null,
                "type": "entityset"
            },
            {
                "name": "entities",
                "default": null,
                "type": "dict"
            },
            {
                "name": "relationships",
                "default": null,
                "type": "list"
            }
        ]
    },
    "produce": {
        "method": "calculate_feature_matrix",
        "args": [
            {
                "name": "X",
                "type": "array"
            },
            {
                "name": "target_entity",
                "default": null,
                "type": "str"
            },
            {
                "name": "entityset",
                "default": null,
                "type": "entityset"
            },
            {
                "name": "entities",
                "default": null,
                "type": "dict"
            },
            {
                "name": "relationships",
                "default": null,
                "type": "list"
            }
        ],
        "output": [
            {
                "name": "X",
                "type": "ndarray"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "target_entity": {
                "type": "str",
                "default": "X"
            },
            "index": {
                "type": "str",
                "default": null
            },
            "time_index": {
                "type": "str",
                "default": null
            },
            "agg_primitives": {
                "type": "list",
                "default": null
            },
            "trans_primitives": {
                "type": "list",
                "default": null
            },
            "copy": {
                "type": "bool",
                "default": false
            }
        },
        "tunable": {
            "max_depth": {
                "type": "int",
                "default": 1,
                "range": [
                    1,
                    3
                ],
                "values": null
            },
            "encode": {
                "type": "bool",
                "default": true
            },
            "remove_low_information": {
                "type": "bool",
                "default": true
            }
        }
    }
}
