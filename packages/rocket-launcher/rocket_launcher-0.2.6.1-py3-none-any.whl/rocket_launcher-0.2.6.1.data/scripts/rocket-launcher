#!python
import os
import threading
import time
from rockets import *

out=''
for i in os.listdir('rockets/bin/'):
    if os.path.isfile(i)==False and i!='__init__.py':
        out=out+"[program:"+i+"]\ncommand = "+i+"\n\n"

f = open("/etc/supervisor.d/rocket-launcher.ini", "w+")
f.write(out)
f.close()

w=threading.Thread(target=supervisord, name='supervisor')
w.start()

multiplexer()
terminal()
ranger()
git()

os.system('sudo -H -u who byobu');
