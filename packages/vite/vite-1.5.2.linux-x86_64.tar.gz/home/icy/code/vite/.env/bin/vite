#!/home/icy/code/vite/.env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'vite==1.5.2','console_scripts','vite'
__requires__ = 'vite==1.5.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('vite==1.5.2', 'console_scripts', 'vite')()
    )
