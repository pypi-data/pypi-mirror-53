#!/Library/Frameworks/Python.framework/Versions/3.7/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'bardolph==0.0.3','console_scripts','lsrun'
__requires__ = 'bardolph==0.0.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('bardolph==0.0.3', 'console_scripts', 'lsrun')()
    )
