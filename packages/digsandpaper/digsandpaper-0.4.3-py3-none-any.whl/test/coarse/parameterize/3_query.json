{
    "SPARQL": {
        "group-by": {
            "limit": 500
        }, 
        "select": {
            "variables": [
                {
                    "type": "owl:Thing", 
                    "variable": "?ad"
                }, 
                {
                    "type": "owl:Thing", 
                    "variable": "?ethnicity"
                }
            ]
        }, 
        "where": {
            "clauses": [
                {
                    "constraint": "1234567890", 
                    "isOptional": false, 
                    "predicate": "phone", 
                    "type": "owl:Thing"
                }, 
                {
                    "constraint": "Toronto, Ontario", 
                    "isOptional": false, 
                    "predicate": "location", 
                    "type": "owl:Thing"
                }, 
                {
                    "isOptional": false, 
                    "predicate": "ethnicity", 
                    "type": "owl:Thing", 
                    "variable": "?ethnicity"
                }, 
                {
                    "isOptional": false, 
                    "predicate": "title", 
                    "type": "owl:Thing", 
                    "variable": "?title"
                }, 
                {
                    "constraint": "blonde", 
                    "isOptional": false, 
                    "predicate": "hairColor", 
                    "type": "owl:Thing"
                }
            ], 
            "filters": [
                {
                    "clauses": [
                        {
                            "constraint": "the enchanting mistress", 
                            "operator": "=", 
                            "type": "owl:Thing", 
                            "variable": "?title"
                        }, 
                        {
                            "constraint": "is available this weekend", 
                            "operator": "=", 
                            "type": "owl:Thing", 
                            "variable": "?content"
                        }
                    ], 
                    "operator": "and"
                }
            ], 
            "type": "Ad", 
            "variable": "?ad"
        }
    }, 
    "type": "Point Fact"
}